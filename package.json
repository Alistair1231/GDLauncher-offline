{
  "name": "gdlauncher",
  "version": "1.0.0",
  "description": "GDLauncher is simple, yet powerful Minecraft custom launcher with a strong focus on the user experience",
  "keywords": [
    "minecraft",
    "launcher",
    "gaming",
    "games",
    "react",
    "redux",
    "reload",
    "sass",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/gorilla-devs/GDLauncher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorilla-devs/GDLauncher.git"
  },
  "license": "MIT",
  "author": {
    "name": "GorillaDevs",
    "email": "info@gdevs.io",
    "url": "https://gdevs.io"
  },
  "private": false,
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development craco start",
    "start-electron": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "build-web": "cross-env-shell APP_TYPE=web craco build",
    "build": "cross-env-shell NODE_ENV=production APP_TYPE=electron craco build",
    "test": "craco test",
    "eject": "craco eject",
    "dev": "cross-env-shell NODE_ENV=development APP_TYPE=electron BROWSER=none \"yarn start-electron\"",
    "dev-web": "cross-env-shell NODE_ENV=development APP_TYPE=web yarn start",
    "postinstall": "electron-builder install-app-deps",
    "release": "cross-env-shell NODE_ENV=production APP_TYPE=electron \"yarn build\" && electron-builder -w --publish never"
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "productName": "GDLauncher",
    "appId": "org.gorilladevs.GDLauncher",
    "files": [
      "!node_modules/**/*",
      "build/**/*",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "squirrelWindows": {
      "iconUrl": "./public/favicon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "artifactName": "${productName}-mac-setup.${ext}"
    },
    "win": {
      "target": [
        "zip"
      ],
      "artifactName": "${productName}-win-setup.${ext}"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Game",
      "artifactName": "${productName}-linux-setup.${ext}"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "gorilla-devs",
      "repo": "GDLauncher",
      "private": false
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "@craco/craco": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^5.0.10",
    "electron-builder": "^21.2.0",
    "electron-builder-squirrel-windows": "^21.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "prettier": "^1.18.2",
    "terser-webpack-plugin": "^2.1.3",
    "wait-on": "^3.3.0",
    "webpack-bundle-analyzer": "^3.5.2"
  },
  "dependencies": {
    "@loadable/babel-plugin": "^5.10.3",
    "@material-ui/core": "^4.5.1",
    "axios": "^0.19.0",
    "bluebird": "^3.7.1",
    "connected-react-router": "^6.5.2",
    "electron-is-dev": "^1.1.0",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "is-electron": "^2.2.0",
    "lodash.memoize": "^4.1.2",
    "lodash.omitby": "^4.6.0",
    "make-dir": "^3.0.0",
    "murmurhash-js": "^1.0.0",
    "p-map": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "reselect": "^4.0.0",
    "rooks": "^3.4.0",
    "semver": "^6.3.0",
    "styled-components": "^4.4.0",
    "typeface-roboto": "^0.0.75",
    "uuid": "^3.3.3"
  }
}
