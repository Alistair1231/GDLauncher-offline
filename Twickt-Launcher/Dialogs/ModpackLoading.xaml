             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Twickt_Launcher.Dialogs"
             mc:Ignorable="d" Background="#FFFFFF" Height="107.823" Width="167.993">
